{
	"name": "spiderwisp/laravel-overlord",
	"description": "A powerful Laravel development console with advanced features including interactive terminal, codebase scanning, database analysis, and optional AI assistant",
	"type": "library",
	"keywords": [
		"laravel",
		"terminal",
		"development",
		"tinker",
		"console",
		"repl",
		"codebase-scanning",
		"database-analysis",
		"ai-assistant",
		"developer-tools"
	],
	"license": "MIT",
	"homepage": "https://laravel-overlord.com",
	"support": {
		"email": "tim@spiderwisp.com"
	},
	"authors": [
		{
			"name": "Spiderwisp",
			"email": "tim@spiderwisp.com"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^12.0",
		"laravel/tinker": "^2.9"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0",
		"orchestra/testbench": "^10.0"
	},
	"autoload": {
		"psr-4": {
			"Spiderwisp\\LaravelOverlord\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Spiderwisp\\LaravelOverlord\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Spiderwisp\\LaravelOverlord\\LaravelOverlordServiceProvider"
			]
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"suggest": {
		"predis/predis": "Required for Redis support (queue features)",
		"laravel/horizon": "Optional: For queue monitoring and job management",
		"vue": "^3.3.4 - Required for frontend Vue components"
	}
}